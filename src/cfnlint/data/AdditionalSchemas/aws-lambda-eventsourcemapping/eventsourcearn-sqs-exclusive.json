{
  "type": "object",
  "description": "When 'EventSourceArn' is associate to SQS don't specify 'StartingPosition'",
  "if": {
    "properties": {
      "EventSourceArn": {
        "pattern": "arn:aws.*:sqs:.*"
      }
    }
  },
  "then": {
    "not": {
      "properties": {
        "StartingPosition": {
          "type": "string"
        }
      },
      "required": ["StartingPosition"]
    }
  },
  "else": {},
  "additionalProperties": true
}
