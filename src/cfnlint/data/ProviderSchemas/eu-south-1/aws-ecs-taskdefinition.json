    "Size"
   ],
   "properties": {
    "ContainerPath": {
     "type": "string"
    },
    "MountOptions": {
     "type": "array",
     "insertionOrder": false,
     "items": {
      "type": "string"
     }
    },
    "Size": {
     "type": "integer"
    }
   },
   "additionalProperties": false
  },
  "Ulimit": {
   "type": "object",
   "required": [
    "HardLimit",
    "Name",
    "SoftLimit"
   ],
   "properties": {
    "HardLimit": {
     "type": "integer"
    },
    "Name": {
     "type": "string"
    },
    "SoftLimit": {
     "type": "integer"
    }
   },
   "additionalProperties": false
  },
  "Volume": {
   "type": "object",
