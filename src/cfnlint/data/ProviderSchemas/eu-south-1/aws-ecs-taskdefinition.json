    "type": "string"
   }
  },
  "TaskRoleArn": {
   "type": "string"
  },
  "Volumes": {
   "type": "array",
   "insertionOrder": false,
   "uniqueItems": true,
   "items": {
    "$ref": "#/definitions/Volume"
   }
  },
  "PidMode": {
   "type": "string"
  },
  "RuntimePlatform": {
   "$ref": "#/definitions/RuntimePlatform"
