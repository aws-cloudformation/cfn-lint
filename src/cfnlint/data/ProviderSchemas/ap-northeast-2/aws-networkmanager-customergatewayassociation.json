  "/properties/CustomerGatewayArn",
  "/properties/DeviceId",
  "/properties/LinkId"
 ],
 "primaryIdentifier": [
  "/properties/GlobalNetworkId",
  "/properties/CustomerGatewayArn"
 ],
 "handlers": {
  "create": {
   "permissions": [
    "networkmanager:GetCustomerGatewayAssociations",
    "networkmanager:AssociateCustomerGateway"
   ]
  },
  "read": {
   "permissions": [
    "networkmanager:GetCustomerGatewayAssociations"
   ]
  },
  "list": {
   "permissions": [
    "networkmanager:GetCustomerGatewayAssociations"
   ]
  },
  "delete": {
