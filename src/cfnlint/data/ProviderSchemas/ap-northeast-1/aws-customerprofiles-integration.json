   },
   "additionalProperties": false
  },
  "SourceFlowConfig": {
   "type": "object",
   "properties": {
    "ConnectorType": {
     "$ref": "#/definitions/ConnectorType"
    },
    "ConnectorProfileName": {
     "type": "string",
     "maxLength": 256,
     "pattern": "[\\w/!@#+=.-]+"
    },
    "IncrementalPullConfig": {
     "$ref": "#/definitions/IncrementalPullConfig"
    },
    "SourceConnectorProperties": {
     "$ref": "#/definitions/SourceConnectorProperties"
    }
   },
   "required": [
    "ConnectorType",
    "SourceConnectorProperties"
   ],
   "additionalProperties": false
