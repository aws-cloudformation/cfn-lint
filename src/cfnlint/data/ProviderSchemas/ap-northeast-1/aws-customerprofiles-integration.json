  "ConnectorType": {
   "type": "string",
   "enum": [
    "Salesforce",
    "Marketo",
    "ServiceNow",
    "Zendesk",
    "S3"
   ]
  },
  "IncrementalPullConfig": {
   "type": "object",
   "properties": {
    "DatetimeTypeFieldName": {
     "type": "string",
     "maxLength": 256
