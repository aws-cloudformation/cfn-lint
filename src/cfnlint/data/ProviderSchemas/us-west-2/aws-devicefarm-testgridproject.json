{
  "typeName" : "AWS::DeviceFarm::TestGridProject",
  "description" : "AWS::DeviceFarm::TestGridProject creates a new TestGrid Project",
  "properties" : {
    "Description" : {
      "type" : "string",
      "minLength" : 1,
      "maxLength" : 2048,
      "pattern" : ".*\\S.*"
    },
    "Name" : {
      "type" : "string",
      "minLength" : 1,
      "maxLength" : 64,
      "pattern" : ".*\\S.*"
    },
    "VpcConfig" : {
      "$ref" : "#/definitions/VpcConfig"
    },
    "Arn" : {
      "type" : "string",
      "minLength" : 32,
      "maxLength" : 1011,
      "pattern" : "^arn:.+"
    },
    "Tags" : {
      "type" : "array",
      "uniqueItems" : false,
      "insertionOrder" : false,
      "items" : {
        "$ref" : "#/definitions/Tag"
      }
    }
  },
  "definitions" : {
    "VpcConfig" : {
      "description" : "The VPC security groups and subnets that are attached to a TestGrid project.",
