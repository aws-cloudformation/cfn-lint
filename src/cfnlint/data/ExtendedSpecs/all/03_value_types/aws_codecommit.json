[
  {
    "op": "add",
    "path": "/ValueTypes/AWS::CodeCommit::Repository.RepositoryTrigger.Events",
    "value": {
      "AllowedValues": [
        "all",
        "createReference",
        "deleteReference",
        "updateReference"
      ]
    }
  },
  {
    "op": "add",
    "path": "/ValueTypes/AWS::CodeCommit::Repository.RepositoryName",
    "value": {
      "AllowedPatternRegex": "^[a-zA-Z0-9._\\-]+(?<!\\.git)$",
      "StringMax": 100,
      "StringMin": 1
    }
  }
]
