{
 "additionalProperties": false,
 "createOnlyProperties": [
  "/properties/ResourceArn"
 ],
 "primaryIdentifier": [
  "/properties/ResourceArn"
 ],
 "properties": {
  "Policy": {
   "format": "json",
   "maxLength": 10000,
   "minLength": 1,
   "pattern": "^.*\\S.*$",
   "type": [
    "object",
    "string"
   ]
  },
  "ResourceArn": {
   "maxLength": 200,
   "minLength": 20,
   "pattern": "^arn(:[a-z0-9]+([.-][a-z0-9]+)*){2}(:([a-z0-9]+([.-][a-z0-9]+)*)?){2}:((servicenetwork/sn)|(service/svc))-[0-9a-z]{17}$",
   "type": "string"
  }
 },
 "required": [
  "ResourceArn",
  "Policy"
 ],
 "tagging": {
  "taggable": false
 },
 "typeName": "AWS::VpcLattice::ResourcePolicy"
}
