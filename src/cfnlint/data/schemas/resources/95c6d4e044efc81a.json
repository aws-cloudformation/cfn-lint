{
 "$schema": "https://raw.githubusercontent.com/aws-cloudformation/cloudformation-resource-schema/master/src/main/resources/schema/provider.definition.schema.v1.json",
 "additionalProperties": false,
 "createOnlyProperties": [
  "/properties/AutoScalingGroupName",
  "/properties/LifecycleHookName"
 ],
 "primaryIdentifier": [
  "/properties/AutoScalingGroupName",
  "/properties/LifecycleHookName"
 ],
 "properties": {
  "AutoScalingGroupName": {
   "maxLength": 255,
   "minLength": 1,
   "type": "string"
  },
  "DefaultResult": {
   "enum": [
    "ABANDON",
    "CONTINUE"
   ],
   "type": "string"
  },
  "HeartbeatTimeout": {
   "type": "integer"
  },
  "LifecycleHookName": {
   "maxLength": 255,
   "minLength": 1,
   "pattern": "[A-Za-z0-9\\-_\\/]+",
   "type": "string"
  },
  "LifecycleTransition": {
   "enum": [
    "autoscaling:EC2_INSTANCE_LAUNCHING",
    "autoscaling:EC2_INSTANCE_TERMINATING"
   ],
   "type": "string"
  },
  "NotificationMetadata": {
   "maxLength": 1023,
   "minLength": 1,
   "pattern": "[\\u0009\\u000A\\u000D\\u0020-\\u007e]+",
   "type": "string"
  },
  "NotificationTargetARN": {
   "maxLength": 255,
   "type": "string"
  },
  "RoleARN": {
   "format": "AWS::IAM::Role.Arn",
   "maxLength": 255,
   "minLength": 1,
   "type": "string"
  }
 },
 "required": [
  "LifecycleTransition",
  "AutoScalingGroupName"
 ],
 "sourceUrl": "https://github.com/aws-cloudformation/aws-cloudformation-resource-providers-autoscaling.git",
 "tagging": {
  "taggable": false
 },
 "typeName": "AWS::AutoScaling::LifecycleHook"
}
