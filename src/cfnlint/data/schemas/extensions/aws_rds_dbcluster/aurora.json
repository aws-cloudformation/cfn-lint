   "Iops": false,
   "MonitoringInterval": false,
   "MonitoringRoleArn": false,
   "PerformanceInsightsEnabled": false,
   "PerformanceInsightsKmsKeyId": false,
   "PerformanceInsightsRetentionPeriod": false,
   "PubliclyAccessible": false,
   "StorageType": {
