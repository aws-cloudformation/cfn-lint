[
 {
  "op": "add",
  "path": "/properties/ModelPackageName/pattern",
  "value": "^[a-zA-Z0-9](-*[a-zA-Z0-9]){0,62}$"
 },
 {
  "op": "add",
  "path": "/properties/ModelPackageGroupName/pattern",
  "value": "(arn:aws[a-z\\-]*:sagemaker:[a-z0-9\\-]*:[0-9]{12}:[a-z\\-]*\\/)?([a-zA-Z0-9]([a-zA-Z0-9-]){0,62})(?<!-)$"
 },
 {
  "op": "add",
  "path": "/properties/ModelPackageDescription/pattern",
  "value": "[\\p{L}\\p{M}\\p{Z}\\p{S}\\p{N}\\p{P}]*"
 },
 {
  "op": "add",
  "path": "/properties/ModelApprovalStatus/enum",
  "value": [
   "Approved",
   "PendingManualApproval",
   "Rejected"
  ]
 },
 {
  "op": "add",
  "path": "/properties/ClientToken/pattern",
  "value": "^[a-zA-Z0-9-]+$"
 },
 {
  "op": "add",
  "path": "/properties/SamplePayloadUrl/pattern",
  "value": "^(https|s3)://([^/]+)/?(.*)$"
 },
 {
  "op": "add",
  "path": "/properties/SkipModelValidation/enum",
  "value": [
   "All",
   "None"
  ]
 },
 {
  "op": "add",
  "path": "/properties/SourceUri/pattern",
  "value": "[\\p{L}\\p{M}\\p{Z}\\p{N}\\p{P}]{0,1024}"
 }
]
