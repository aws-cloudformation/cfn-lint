[
 {
  "op": "add",
  "path": "/properties/Name/pattern",
  "value": "[a-zA-Z][0-9a-zA-Z-_]*"
 },
 {
  "op": "add",
  "path": "/properties/ExecutionRoleArn/pattern",
  "value": "arn:aws(-[a-z]+)?:iam::\\d{12}:role/?[a-zA-Z_0-9+=,.@\\-_/]+"
 },
 {
  "op": "add",
  "path": "/properties/SourceBucketArn/pattern",
  "value": "arn:aws(-[a-z]+)?:s3:::[a-z0-9.\\-]+"
 },
 {
  "op": "add",
  "path": "/properties/KmsKey/pattern",
  "value": "(((arn:aws(-[a-z]+)?:kms:[a-z]{2}-[a-z]+-\\d:\\d+:)?key\\/)?[a-f0-9]{8}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{12}|(arn:aws(-[a-z]+)?:kms:[a-z]{2}-[a-z]+-\\d:\\d+:)?alias/.+)"
 },
 {
  "op": "add",
  "path": "/properties/AirflowVersion/pattern",
  "value": "[0-9a-z.]+"
 },
 {
  "op": "add",
  "path": "/properties/WeeklyMaintenanceWindowStart/pattern",
  "value": ".*(MON|TUE|WED|THU|FRI|SAT|SUN):([01]\\d|2[0-3]):(00|30).*"
 },
 {
  "op": "add",
  "path": "/properties/WebserverAccessMode/enum",
  "value": [
   "PRIVATE_ONLY",
   "PUBLIC_ONLY"
  ]
 },
 {
  "op": "add",
  "path": "/properties/EndpointManagement/enum",
  "value": [
   "CUSTOMER",
   "SERVICE"
  ]
 }
]
