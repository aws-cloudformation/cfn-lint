    },
    "ParentActionGroupSignature": {
     "$ref": "#/definitions/ActionGroupSignature"
    },
    "SkipResourceInUseCheckOnDelete": {
     "default": false,
     "type": "boolean"
    }
   },
   "required": [
    "ActionGroupName"
   ],
   "type": "object"
  },
  "AgentKnowledgeBase": {
   "additionalProperties": false,
   "properties": {
    "Description": {
     "maxLength": 200,
     "minLength": 1,
     "type": "string"
