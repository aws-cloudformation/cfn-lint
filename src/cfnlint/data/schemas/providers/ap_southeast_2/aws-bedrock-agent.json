     },
     "type": "array"
    }
   },
   "required": [
    "Functions"
   ],
   "title": "Functions",
   "type": "object"
  },
  "GuardrailConfiguration": {
   "additionalProperties": false,
   "properties": {
    "GuardrailIdentifier": {
     "maxLength": 2048,
     "pattern": "^(([a-z0-9]+)|(arn:aws(-[^:]+)?:bedrock:[a-z0-9-]{1,20}:[0-9]{12}:guardrail/[a-z0-9]+))$",
     "type": "string"
    },
    "GuardrailVersion": {
     "pattern": "^(([0-9]{1,8})|(DRAFT))$",
     "type": "string"
    }
   },
   "type": "object"
  },
  "InferenceConfiguration": {
   "additionalProperties": false,
   "properties": {
    "MaximumLength": {
     "maximum": 4096,
     "minimum": 0,
     "type": "number"
    },
    "StopSequences": {
     "insertionOrder": false,
     "items": {
      "type": "string"
     },
     "maxItems": 4,
     "minItems": 0,
     "type": "array"
    },
    "Temperature": {
     "maximum": 1,
     "minimum": 0,
     "type": "number"
    },
    "TopK": {
     "maximum": 500,
     "minimum": 0,
     "type": "number"
    },
    "TopP": {
     "maximum": 1,
     "minimum": 0,
     "type": "number"
    }
   },
   "type": "object"
  },
  "KnowledgeBaseState": {
   "enum": [
    "ENABLED",
    "DISABLED"
   ],
   "type": "string"
  },
  "ParameterDetail": {
   "additionalProperties": false,
   "properties": {
    "Description": {
