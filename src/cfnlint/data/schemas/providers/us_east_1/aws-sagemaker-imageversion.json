   "pattern": "^arn:aws(-[\\w]+)*:sagemaker:[a-z0-9\\-]*:[0-9]{12}:image\\/[a-zA-Z0-9]([-.]?[a-zA-Z0-9])*$",
   "type": "string"
  },
  "ImageName": {
   "description": "The name of the image this version belongs to.",
   "maxLength": 63,
   "minLength": 1,
   "pattern": "^[A-Za-z0-9]([-.]?[A-Za-z0-9])*$",
   "type": "string"
  },
