 "readOnlyProperties": [
  "/properties/ApplicationArn",
  "/properties/ApplicationId",
  "/properties/CreatedAt",
  "/properties/IdentityCenterApplicationArn",
  "/properties/Status",
  "/properties/UpdatedAt"
