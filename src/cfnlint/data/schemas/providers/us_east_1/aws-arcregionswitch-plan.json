 "properties": {
  "Arn": {
   "pattern": "^arn:aws[a-zA-Z-]*:arc-region-switch::[0-9]{12}:plan/([a-zA-Z0-9](?:[a-zA-Z0-9-]{0,30}[a-zA-Z0-9])?):([a-z0-9]{6})$",
   "type": "string"
  },
  "AssociatedAlarms": {
   "$ref": "#/definitions/AssociatedAlarmMap"
  },
  "Description": {
   "type": "string"
  },
  "ExecutionRole": {
   "pattern": "^arn:aws[a-zA-Z0-9-]*:iam::[0-9]{12}:role/.+$",
   "type": "string"
  },
  "HealthChecksForPlan": {
   "additionalProperties": false,
   "patternProperties": {
    ".+": {
     "$ref": "#/definitions/HealthChecks"
    }
   },
   "type": "object"
  },
  "Name": {
   "maxLength": 32,
   "minLength": 1,
   "pattern": "^[a-zA-Z0-9](?:[a-zA-Z0-9-]{0,30}[a-zA-Z0-9])?$",
   "type": "string"
  },
  "Owner": {
   "pattern": "^\\d{12}$",
   "type": "string"
  },
  "PrimaryRegion": {
   "pattern": "^[a-z]{2}-[a-z-]+-\\d+$",
   "type": "string"
  },
  "RecoveryApproach": {
   "$ref": "#/definitions/RecoveryApproach"
  },
  "RecoveryTimeObjectiveMinutes": {
   "maximum": 10080,
   "minimum": 1,
   "type": "number"
  },
  "Regions": {
   "items": {
    "pattern": "^[a-z]{2}-[a-z-]+-\\d+$",
    "type": "string"
   },
   "maxItems": 2,
   "minItems": 2,
   "type": "array"
  },
  "Route53HealthChecks": {
   "additionalProperties": false,
   "properties": {
    "HealthCheckIds": {
     "$ref": "#/definitions/HealthCheckIds"
    },
    "HostedZoneIds": {
     "$ref": "#/definitions/HostedZoneIds"
    },
    "RecordNames": {
     "$ref": "#/definitions/HealthCheckRecordNames"
    },
    "Regions": {
     "$ref": "#/definitions/HealthCheckRegions"
    }
   },
   "type": "object"
  },
  "Tags": {
   "$ref": "#/definitions/Tags"
  },
  "Triggers": {
   "items": {
    "$ref": "#/definitions/Trigger"
   },
   "type": "array"
  },
  "Version": {
   "type": "string"
  },
  "Workflows": {
   "items": {
    "$ref": "#/definitions/Workflow"
   },
   "type": "array"
  }
 },
 "readOnlyProperties": [
  "/properties/Arn",
  "/properties/Owner",
  "/properties/Version",
  "/properties/HealthChecksForPlan",
  "/properties/Route53HealthChecks"
 ],
 "required": [
  "ExecutionRole",
  "Name",
  "RecoveryApproach",
  "Regions",
  "Workflows"
 ],
 "tagging": {
  "cloudFormationSystemTags": true,
  "tagOnCreate": true,
  "tagProperty": "/properties/Tags",
  "tagUpdatable": true,
  "taggable": true
 },
 "typeName": "AWS::ARCRegionSwitch::Plan"
}
