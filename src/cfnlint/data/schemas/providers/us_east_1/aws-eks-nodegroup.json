     "uniqueItems": false
    }
   },
   "required": [
    "Ec2SshKey"
   ],
   "type": "object"
  },
  "ScalingConfig": {
   "additionalProperties": false,
   "properties": {
    "DesiredSize": {
     "minimum": 0,
     "type": "integer"
