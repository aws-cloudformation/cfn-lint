   "maxLength": 512,
   "pattern": "arn:aws:appflow:.*:[0-9]+:.*",
   "type": "string"
  },
  "FlowName": {
   "description": "Name of the flow.",
   "maxLength": 256,
   "minLength": 1,
   "pattern": "[a-zA-Z0-9][\\w!@#.-]+",
   "type": "string"
  },
