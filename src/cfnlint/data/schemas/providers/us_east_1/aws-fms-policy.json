  },
  "ResourceArn": {
   "maxLength": 1024,
   "minLength": 1,
   "pattern": "^([^\\s]*)$",
   "type": "string"
  },
  "ResourceTag": {
   "additionalProperties": false,
   "properties": {
    "Key": {
     "maxLength": 128,
     "minLength": 1,
