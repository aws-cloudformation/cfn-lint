    {
     "required": [
      "LastEntries"
     ]
    }
   ],
   "properties": {
    "FirstEntries": {
     "$ref": "#/definitions/NetworkAclEntryList"
    },
    "ForceRemediateForFirstEntries": {
     "type": "boolean"
