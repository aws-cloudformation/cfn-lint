   },
   "maxItems": 50,
   "type": "array",
   "uniqueItems": true
  },
  "WorkflowId": {
   "maxLength": 19,
   "minLength": 19,
   "pattern": "^w-([a-z0-9]{17})$",
   "type": "string"
  }
 },
 "readOnlyProperties": [
  "/properties/WorkflowId",
  "/properties/Arn"
 ],
 "required": [
