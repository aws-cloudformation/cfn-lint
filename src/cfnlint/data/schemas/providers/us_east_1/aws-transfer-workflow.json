      "SourceFileLocation": {
       "maxLength": 256,
       "minLength": 0,
       "pattern": "^\\$\\{(\\w+.)+\\w+\\}$",
       "type": "string"
      }
     },
     "type": "object"
    },
    "TagStepDetails": {
     "additionalProperties": false,
     "properties": {
      "Name": {
       "maxLength": 30,
       "minLength": 0,
       "pattern": "^[\\w-]*$",
       "type": "string"
      },
      "SourceFileLocation": {
       "maxLength": 256,
       "minLength": 0,
       "pattern": "^\\$\\{(\\w+.)+\\w+\\}$",
       "type": "string"
      },
      "Tags": {
       "insertionOrder": false,
       "items": {
        "$ref": "#/definitions/S3Tag"
       },
       "maxItems": 10,
       "type": "array",
       "uniqueItems": true
      }
     },
     "type": "object"
    },
    "Type": {
     "enum": [
      "COPY",
      "CUSTOM",
      "DECRYPT",
      "DELETE",
      "TAG"
     ],
     "type": "string"
    }
   },
   "type": "object"
  }
 },
