    },
    "FailureTolerancePercentage": {
     "maximum": 100,
     "minimum": 0,
     "type": "integer"
    },
    "MaxConcurrentCount": {
     "minimum": 1,
     "type": "integer"
    },
    "MaxConcurrentPercentage": {
     "maximum": 100,
     "minimum": 0,
     "type": "integer"
    },
