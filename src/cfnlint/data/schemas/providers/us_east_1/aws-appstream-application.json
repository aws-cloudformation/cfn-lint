      }
     },
     "required": [
      "Key",
      "Value"
     ],
     "type": "object"
    },
    {
     "additionalProperties": false,
     "properties": {
      "TagKey": {
       "type": "string"
      },
      "TagValue": {
       "type": "string"
      }
     },
     "required": [
      "TagKey",
      "TagValue"
     ],
     "type": "object"
    }
   ]
  }
 },
