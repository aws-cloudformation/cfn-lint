   "type": "boolean"
  },
  "ReplicaMode": {
   "type": "string"
  },
  "RestoreTime": {
   "format": "date-time",
   "type": "string"
  },
  "SourceDBClusterIdentifier": {
   "type": "string"
  },
  "SourceDBInstanceAutomatedBackupsArn": {
   "type": "string"
  },
  "SourceDBInstanceIdentifier": {
   "type": "string"
