  },
  "DBSnapshotIdentifier": {
   "type": "string"
  },
  "DBSubnetGroupName": {
   "type": "string"
  },
  "DBSystemId": {
   "type": "string"
  },
  "DbiResourceId": {
   "type": "string"
  },
  "DedicatedLogVolume": {
   "type": "boolean"
  },
  "DeleteAutomatedBackups": {
   "type": "boolean"
  },
  "DeletionProtection": {
   "type": "boolean"
  },
  "Domain": {
