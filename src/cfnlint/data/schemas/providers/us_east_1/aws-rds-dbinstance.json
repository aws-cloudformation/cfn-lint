  "DomainDnsIps": {
   "items": {
    "type": "string"
   },
   "type": "array"
  },
  "DomainFqdn": {
   "type": "string"
  },
  "DomainIAMRoleName": {
   "type": "string"
  },
  "DomainOu": {
   "type": "string"
  },
  "EnableCloudwatchLogsExports": {
   "items": {
    "type": "string"
   },
   "type": "array"
  },
  "EnableIAMDatabaseAuthentication": {
   "type": "boolean"
  },
  "EnablePerformanceInsights": {
   "type": "boolean"
  },
  "Endpoint": {
   "$ref": "#/definitions/Endpoint"
  },
  "Engine": {
   "type": "string"
  },
  "EngineLifecycleSupport": {
   "type": "string"
  },
  "EngineVersion": {
   "type": "string"
  },
  "Iops": {
   "type": "integer"
  },
  "KmsKeyId": {
   "type": "string"
  },
  "LicenseModel": {
   "type": "string"
  },
  "ManageMasterUserPassword": {
   "type": "boolean"
