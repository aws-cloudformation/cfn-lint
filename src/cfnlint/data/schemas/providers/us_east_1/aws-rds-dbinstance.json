  "/properties/DBClusterSnapshotIdentifier",
  "/properties/DBParameterGroupName",
  "/properties/DBSnapshotIdentifier",
  "/properties/Engine",
  "/properties/MultiAZ",
  "/properties/PerformanceInsightsKMSKeyId",
