   "type": "string"
  },
  "Subdirectory": {
   "maxLength": 1024,
   "pattern": "^[\\p{L}\\p{M}\\p{Z}\\p{S}\\p{N}\\p{P}\\p{C}]*$",
   "type": "string"
  },
  "Tags": {
   "insertionOrder": false,
   "items": {
