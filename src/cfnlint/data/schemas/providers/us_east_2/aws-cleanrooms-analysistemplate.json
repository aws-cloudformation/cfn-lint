     "required": [
      "Text"
     ],
     "title": "Text",
     "type": "object"
    },
    {
     "additionalProperties": false,
     "properties": {
      "Artifacts": {
       "$ref": "#/definitions/AnalysisTemplateArtifacts"
      }
     },
     "required": [
      "Artifacts"
     ],
     "title": "Artifacts",
     "type": "object"
    }
   ]
  },
  "AnalysisSourceMetadata": {
   "oneOf": [
    {
     "additionalProperties": false,
     "properties": {
      "Artifacts": {
       "$ref": "#/definitions/AnalysisTemplateArtifactMetadata"
      }
     },
     "required": [
      "Artifacts"
     ],
     "title": "Artifacts",
     "type": "object"
    }
   ]
  },
  "AnalysisTemplateArtifact": {
