     "maxLength": 100,
     "minLength": 1,
     "pattern": "[0-9a-zA-Z_]+",
     "type": "string"
    },
    "Type": {
     "enum": [
      "SMALLINT",
      "INTEGER",
      "BIGINT",
      "DECIMAL",
      "REAL",
      "DOUBLE_PRECISION",
      "BOOLEAN",
      "CHAR",
      "VARCHAR",
      "DATE",
      "TIMESTAMP",
      "TIMESTAMPTZ",
      "TIME",
      "TIMETZ",
      "VARBYTE",
      "BINARY",
      "BYTE",
      "CHARACTER",
      "DOUBLE",
      "FLOAT",
      "INT",
      "LONG",
      "NUMERIC",
      "SHORT",
      "STRING",
      "TIMESTAMP_LTZ",
      "TIMESTAMP_NTZ",
      "TINYINT"
     ],
     "type": "string"
    }
   },
   "required": [
