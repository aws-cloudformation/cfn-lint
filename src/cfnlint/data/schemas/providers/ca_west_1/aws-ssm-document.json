    "$ref": "#/definitions/Tag"
   },
   "maxItems": 1000,
   "type": "array"
  },
  "TargetType": {
   "pattern": "^\\/[\\w\\.\\-\\:\\/]*$",
   "type": "string"
  },
  "UpdateMethod": {
   "default": "Replace",
   "enum": [
    "Replace",
    "NewVersion"
   ],
   "type": "string"
  },
  "VersionName": {
   "pattern": "^[a-zA-Z0-9_\\-.]{1,128}$",
   "type": "string"
  }
 },
 "required": [
  "Content"
 ],
 "sourceUrl": "https://github.com/aws-cloudformation/aws-cloudformation-resource-providers-ssm",
 "tagging": {
